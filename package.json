{
  "name": "mean-ionic-ngrx",
  "author": "Nicolas Fazio",
  "homepage": "http://nicoalsfazio.ch/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "postinstall": "node tools/install.js && npm run helper.cmd",
    "prod": "NODE_ENV=prod ionic serve --prod",
    "dev": "npm run server:dev & ionic serve --dev",
    "server:prod-tsc": "tsc server/server.ts --outDir ./platforms/server && NODE_ENV=prod node ./platforms/server/server.js ",
    "server:prod": "npm run build:server && NODE_ENV=prod node ./platforms/server/server.js ",
    "server:dev-tsc": "npm run mongod & tsc server/server.ts -w --outDir ./platforms/server & nodemon ./platforms/server/server.js --dev  --ignore src/ --ignore www/ --ignore tools/ --ignore .tmp/",
    "server:dev": "npm run mongod & webpack --watch --config ./tools/config/server.webpack.js --env.dev & nodemon --config ./tools/config/server.nodemon.json ./platforms/server/server.js",
    "mongod": "$HOME/Applications/mongodb/bin/mongod --dbpath $HOME/Applications/mongodb/data/db",
    "deploy:client": "npm run build:browser && gulp deploy",
    "deploy:server": "npm run build:server && node ./tools/tasks/deploy.server.js",
    "build:server": "NODE_ENV=prod webpack --config ./tools/config/server.webpack.js --env.prod",
    "build:browser": "NODE_ENV=prod ionic cordova build browser --prod",
    "build:ios": "NODE_ENV=prod ionic cordova build ios --prod",
    "docs": "npm run typedoc -- --out docs/ --exclude '**/*+(e2e|spec|index).ts' ./src/  && open docs/index.html",
    "typedoc": "typedoc",
    "helper.cmd": "node ./tools/helper.cmd.js",
    "e2e": "protractor ./tools/config/test/protractor.conf.js",
    "test": "karma start ./tools/config/test/karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@ionic-native/core": "4.3.0",
    "@ionic-native/splash-screen": "4.3.0",
    "@ionic-native/status-bar": "4.3.0",
    "@ionic/storage": "2.0.1",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "apollo-angular": "^0.13.2",
    "apollo-client": "^1.9.3",
    "apollo-link": "^0.7.0",
    "apollo-link-http": "^0.7.0",
    "apollo-link-ws": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-server-express": "^1.1.3",
    "graphql-subscriptions": "^0.5.4",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^2.2.1",
    "ionic-angular": "3.7.1",
    "ionicons": "3.0.0",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.27",
    "mongoose": "^4.10.0",
    "morgan": "^1.8.2",
    "ngrx-store-freeze": "^0.1.9",
    "rxjs": "5.4.3",
    "subscriptions-transport-ws": "^0.9.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.0.0",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/store": "^4.0.5",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/body-parser": "^1.16.3",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/graphql": "^0.11.5",
    "@types/jasmine": "^2.5.47",
    "@types/mongodb": "^2.2.2",
    "@types/mongoose": "^4.7.13",
    "@types/node": "^8.0.32",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "connect": "^3.6.1",
    "copy-webpack-plugin": "^4.1.1",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "html-loader": "^0.5.1",
    "ionic": "3.13.0",
    "ionic-mocks": "^1.0.3",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mongoose": "^4.12.0",
    "ngrx-store-freeze": "^0.2.0",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "protractor": "^5.1.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0",
    "typescript": "~2.3.4",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "~2.9.0",
    "webpack-dev-middleware": "~1.12.0",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "~4.1.0"
  },
  "engines": {
    "node": "~7.0.0"
  },
  "version": "0.0.5",
  "config": {
    "ionic_purge_decorators": false,
    "ionic_webpack": "./tools/config/ionic.webpack.js",
    "ionic_sass": "./tools/config/ionic.sass.js"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-statusbar",
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ],
  "description": "Full MEAN stack with Ionic Framework and ReactiveX API (ngrx/Store + ngrx/Effects)"
}
